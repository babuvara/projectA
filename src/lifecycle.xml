<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans.xsd 
  http://www.springframework.org/schema/context/ 
  http://www.springframework.org/schema/context/spring-context.xsd">
	<bean id="lcb"
		class="com.chainsys.springproject.lifecycle.LifeCycleBean"
		init-method="setUp" destroy-method="close"></bean>
		<bean id="init"
		class="com.chainsys.springproject.lifecycle.InitDispossBean"
		init-method="setUp" destroy-method="close">
		<property name="City" value="salem"></property>
		<property name="Pincode" value="636006"></property>
		</bean>
		<bean id="annLc"
		class="com.chainsys.springproject.lifecycle.AnnotationLifeCycle"></bean>
</beans>